<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <solid android:color="@color/transparent_primary_20" />

    <stroke android:color="?attr/colorPrimary"
        android:width="1dp"
        android:dashGap="4dp"
        android:dashWidth="4dp" />

    <corners android:topRightRadius="0dp"
        android:topLeftRadius="0dp"
        android:bottomLeftRadius="@dimen/corner_size_medium"
        android:bottomRightRadius="@dimen/corner_size_medium" />
</shape>

